{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-debugbar": "^3.2",
        "chumper/zipper": "^1.0",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.8",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "pickles2/lib-indigo": "dev-develop",
        "pickles2/lib-plum": "dev-develop",
        "pickles2/px2style": "^2.0",
        "predis/predis": "^1.1",
        "tomk79/filesystem": "^1.0",
        "tomk79/remote-finder": "^0.0.3",
        "twig/twig": "2.x-dev"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
            "app/helpers/locale_url.php",
            "app/helpers/get_project_dir.php",
            "app/helpers/get_project_workingtree_dir.php",
            "app/helpers/get_git_remote_default_branch_name.php",
            "app/helpers/git.php",
            "app/helpers/composer.php",
			"app/helpers/px2query.php",
			"app/helpers/get_px_execute_path.php",
			"app/helpers/get_path_homedir.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
